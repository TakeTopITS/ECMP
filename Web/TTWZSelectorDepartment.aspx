    <script type="text/jscript">

        //单选择按钮判断
        function chooseUpdate() {
            var str = "";
            $("input[name=name1]").each(function () {
                if (this.checked) {
                    str = $(this).attr("id");
                }
            });

            if (str == "") {
                alert('请选择记录项！');
                return false;
            }
            else {
                clickMember(str);
            }
        }

        function clickMember(strvalue) {


            if (strvalue != "" && strvalue != undefined) {

              window.parent.document.getElementById("TXT_UnitName").value = strvalue;

            }

            CloseLayer();
        }

    </script>
</head>
