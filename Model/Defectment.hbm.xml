<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">

  <class name="ProjectMgt.Model.Defectment, ProjectMgt.Model" table="T_Defectment">

    <id name="DefectID" column="DefectID" type="int">

      <generator class="identity" />
    </id>

    <property name ="DefectType" type ="String" />

    <property name ="DefectName"  type ="String" />

    <property name ="DefectDetail" type ="String" />

    <property name ="AcceptStandard" type ="String" />

    <property name ="ApplicantCode" type ="String" />

    <property name ="ApplicantName" type ="String" />

    <property name ="MakeDate" type ="DateTime" />

    <property name ="DefectFinishedDate" type ="DateTime" />

    <property name ="FileAddress" type ="String" />

    <property name ="Status" type ="String" />

    <property name ="RouteNumber" type ="int" />
  </class>
</hibernate-mapping>